  "license": "MIT",
  "devDependencies": {
    "rimraf": "^2.5.2",
    "ts-node": "^0.7.2",
    "typescript": "^1.8.10",
    "typings": "^0.8.1"
  },
  "dependencies": {
    "core-js": "^2.3.0"
  },
  "files": [
    "lib"
  ]
}
