{
  "name": "signal-tree",
  "version": "0.1.0",
  "description": "A JavaScript execution controller",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib && rimraf tests/*.js*",
    "prebuild": "typings install",
